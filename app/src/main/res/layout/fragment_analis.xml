<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".AnalisFragment">

    <com.github.mikephil.charting.charts.LineChart
        android:id="@+id/lineChart"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/all"
        />

    <TextView
        android:id="@+id/all"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Кількість симптомів"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/lineChart"
        app:layout_constraintBottom_toTopOf="@id/l10"/>

    <TextView
        android:id="@+id/l10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Кількість 10 рівня"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/all"
        app:layout_constraintBottom_toTopOf="@id/l9"/>

    <TextView
        android:id="@+id/l9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Кількість 9 рівня"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/l10"
        app:layout_constraintBottom_toTopOf="@id/l8"/>

    <TextView
        android:id="@+id/l8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Кількість 8 рівня"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/l9"
        app:layout_constraintBottom_toTopOf="@id/l7"/>

    <TextView
        android:id="@+id/l7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Кількість 7 рівня"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/l8"
        app:layout_constraintBottom_toTopOf="@id/l6"/>

    <TextView
        android:id="@+id/l6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Кількість 6 рівня"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/l7"
        app:layout_constraintBottom_toTopOf="@id/l5"/>

    <TextView
        android:id="@+id/l5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Кількість 5 рівня"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/l6"
        app:layout_constraintBottom_toTopOf="@id/l4"/>

    <TextView
        android:id="@+id/l4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Кількість 4 рівня"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/l5"
        app:layout_constraintBottom_toTopOf="@id/l3" />

    <TextView
        android:id="@+id/l3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Кількість 3 рівня"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/l4"
        app:layout_constraintBottom_toTopOf="@id/l2"/>

    <TextView
        android:id="@+id/l2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Кількість 2 рівня"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/l3"
        app:layout_constraintBottom_toTopOf="@id/l1"/>

    <TextView
        android:id="@+id/l1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Кількість 1 рівня"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/l2"
        app:layout_constraintBottom_toTopOf="@id/l0"
        />
    <TextView
        android:id="@+id/l0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Кількість 0 рівня"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/l1" />



</androidx.constraintlayout.widget.ConstraintLayout>